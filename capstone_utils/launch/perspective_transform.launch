<launch>
    <!-- lane detection camera -->
    <node name="usb_cam_perspective_transform" pkg="usb_cam" type="usb_cam_node" output="screen" >
        <param name="video_device" value="/dev/video1" />
        <param name="image_width" value="864" />
        <param name="image_height" value="480" />
        <param name="pixel_format" value="yuyv" />
        <param name="camera_frame_id" value="usb_cam" />
        <param name="io_method" value="mmap"/>
    </node>
	
	<node name="perspective_transform_node" pkg="capstone_utils" type="perspective_transform_node" output="screen">
	</node>
</launch>
