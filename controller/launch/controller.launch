<launch>
    <node pkg="joy" type="joy_node" name="joy_node"/>
    <node pkg="controller" type="controller_node" name="controller_node" output="screen">
        <param name="accel" type="double" value="0.2"/>
        <param name="steer_offset" type="double" value="0.00"/>

        <param name="p_gain" type="double" value="0.6"/>
        <param name="i_gain" type="double" value="0.001"/>
        <param name="d_gain" type="double" value="2"/>

        <param name="menual_start" type="bool" value="true"/>
    </node>
</launch>
