<launch>
    <node pkg="obstacle_detection" type="obstacle_detection_node" name="obstacle_detection_node">
        <param name="box_height" type="double" value="4" />
        <param name="box_width" type="double" value="2" />

        <param name="cluster_tolerance" type="double" value="0.1" />
